{
  "_version": "1.65.0",
  "sap.app": {
    "id": "customdgfiori",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.3",
      "toolsId": "948929d7-e39e-4f7f-9899-2bc291763352"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/financeservice/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      },
      "authService": {
        "uri": "/auth/",      
        "type": "OData",     
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.138.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.comp": {},
        "sap.ui.layout": {},
        "sap.viz": {},
        "sap.ui.vbm": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "customdgfiori.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "authService": {
        "dataSource": "authService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlId": "appContainer",
        "controlAggregation": "pages",
        "transition": "slide",
        "viewType": "XML",
        "async": true,
        "bypassed": {
            "target": "home"
        }
      },
      "routes": [
        {
            "name": "home",
            "pattern": "",
            "target": "home"
        },
        {
          "name": "login",
          "pattern": "login",
          "target": "login"
        },
        {
          "name": "register",
          "pattern": "register",
          "target": "register"
        },
        {
          "name": "RouteView1",
          "pattern": "view1",
          "target": "TargetView1"
        },
        {
          "name": "ListReport",
          "pattern": "list",
          "target": "ListReport"
      },
      {
          "name": "CreateForm",
          "pattern": "create",
          "target": "CreateForm"
      },
      {
        "pattern": "EditForm/{id}",
        "name": "EditForm",
        "target": "EditForm"
      },      
      {
        "pattern": "Detail/{id}",
        "name": "Detail",
        "target": "Detail"
      },
      {
        "name": "Chart",
        "pattern": "chart",
        "target": "Chart"
      },
      {
        "pattern": "ChartDefault",
        "name": "ChartDefault",
        "target": "ChartDefault"
      },
      {
        "name": "Map",
        "pattern": "map",
        "target": "Map"
      },
      {
        "name": "weather",
        "pattern": "weather",
        "target": "weather"
      },
      {
        "name": "exchange",
        "pattern": "exchange",
        "target": "exchange"
      },
      {
        "pattern": "editProfile",
        "name": "editProfile",
        "target": "editProfile"
      }
      ],
      "targets": {
        "home": {
            "viewName": "customdgfiori.view.Home"
        },
        "login": {
          "viewName": "customdgfiori.view.Auth.Login"
        },
        "register": {
          "viewName": "customdgfiori.view.Auth.Register"
        },
        "TargetView1": {
          "viewName": "customdgfiori.view.View1"
        },
        "ListReport": {
           "viewName": "customdgfiori.view.ListReport"  
        },
        "CreateForm": {
          "viewName": "customdgfiori.view.CreateForm"
        },
        "Detail": {
          "viewName": "customdgfiori.view.Detail"
        },
        "EditForm":{
          "viewName": "customdgfiori.view.EditForm"
        },
        "Chart": {
          "viewName": "customdgfiori.view.Chart"
        },
        "ChartDefault": {
          "viewName": "customdgfiori.view.ChartDefault"
        },
        "Map": {
          "viewName": "customdgfiori.view.Map",
          "viewType": "XML"
        },
        "weather": {
          "viewName": "customdgfiori.view.Weather",
          "title": "Weather Info"
        },
        "exchange": {
          "viewName": "customdgfiori.view.ExchangeView"
        },
        "editProfile": {
          "viewName": "customdgfiori.view.EditProfile"
        }
      }
    },
    "rootView": {
      "viewName": "customdgfiori.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
