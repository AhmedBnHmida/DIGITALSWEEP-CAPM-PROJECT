<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0"
    xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
    xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
    xmlns:sap="http://www.sap.com/Protocols/SAPData">
    <edmx:DataServices m:DataServiceVersion="2.0">
        <Schema Namespace="FinanceService"
            xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
            <EntityType Name="Finances" sap:semantics="aggregate">
                <Key>
                    <PropertyRef Name="Country"/>
                </Key>
                <Property Name="Country" Type="Edm.String" sap:aggregation-role="dimension" sap:label="Country"/>
                <Property Name="Segment" Type="Edm.String" sap:aggregation-role="dimension" sap:label="Segment"/>
                <Property Name="Sales" Type="Edm.Decimal" sap:aggregation-role="measure" sap:label="Sales"/>
                <Property Name="Profit" Type="Edm.Decimal" sap:aggregation-role="measure" sap:label="Profit"/>
            </EntityType>

            <EntityContainer Name="FinanceService_Entities" m:IsDefaultEntityContainer="true">
                <EntitySet Name="Finances" EntityType="FinanceService.Finances"
                    sap:creatable="false" sap:updatable="false" sap:deletable="false"/>
            </EntityContainer>

            <Annotations Target="FinanceService.Finances"
                xmlns="http://docs.oasis-open.org/odata/ns/edm">
                <Annotation Term="com.sap.vocabularies.UI.v1.Chart">
                    <Record>
                        <PropertyValue Property="ChartType"
                            EnumMember="com.sap.vocabularies.UI.v1.ChartType/Column" />
                        <PropertyValue Property="Dimensions">
                            <Collection>
                                <PropertyPath>Country</PropertyPath>
                                <PropertyPath>Segment</PropertyPath>
                            </Collection>
                        </PropertyValue>
                        <PropertyValue Property="Measures">
                            <Collection>
                                <PropertyPath>Sales</PropertyPath>
                                <PropertyPath>Profit</PropertyPath>
                            </Collection>
                        </PropertyValue>
                    </Record>
                </Annotation>
            </Annotations>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
